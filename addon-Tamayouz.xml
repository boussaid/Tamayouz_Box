  <templates>
    <template title="tama_box" version_id="1" version_string="1.1"><![CDATA[<xen:if is="{$tamz.cansee}">
<xen:require css="tama_box.css" />
<xen:include template="preview_tooltip" />

<div class="sectionMain" style="padding:7px"> 
<h3 class="heading">{$xenOptions.pest_all_perday_name}</h3>	
<table class="dataTable">
    <tbody>

    <tr class="dataRow">
         <th>العضو المميز</th>
        <th>الموضوع المميز</th>
<xen:if is="{$xenOptions.altmiz_group1}">
          <th>{$xenOptions.altmiz_group1}</th></xen:if>
        </tr>
        <tr class="dataRow">

<td class="tavatarList" style="width:30%"><xen:if is="{$tamz.Tusr}">
<xen:foreach loop="$tamz.Tusr" value="$usr">
<xen:avatar user="$usr" size="m" img="true" />
	<div class="visitorText">
			<xen:username user="$usr" rich="true" />
<div class="userTitle">{xen:helper userTitle, $usr}</div><br />		
			<div class="stats">
				<dl class="pairsJustified"><dt>{xen:phrase messages}: {xen:number $usr.message_count}</dt></dl>
				<dl class="pairsJustified"><dt>{xen:phrase likes}: {xen:number $usr.like_count}</dt></dl>
				<dl class="pairsJustified"><dt>{xen:phrase points}: {xen:number $usr.trophy_points}</dt></dl>
			</div>
		</div></xen:foreach>
<xen:else />
قريباً
</xen:if>	

</td>

        <td class="tavatarList" style="width:40%"><xen:if is="{$tamz.Threads}">
<xen:foreach loop="$tamz.Threads" value="$thread">
<xen:avatar user="$thread" size="m" img="true" />
<a class="PreviewTooltip" href="{xen:link threads/unread, $thread}" data-previewUrl="{xen:link threads/preview, $thread}">{xen:helper snippet, $thread.title, {$xenOptions.tam_cut}}</a>
<br /><br />
<dl class="pairsJustified"><dt>{xen:datetime $thread.post_date, html} - {xen:phrase views}: {$thread.view_count}  - 
 {xen:phrase replies}: {$thread.reply_count}</dt></dl>
<br />
بقلم:&nbsp;<xen:username user="$thread" rich="true" /> 
</xen:foreach><xen:else />
قريباً
</xen:if>
</td>
<xen:if is="{$xenOptions.altmiz_group1}">
<td class="tavatarList" style="width:30%"><xen:if is="{$tamz.Tmod}">
<xen:foreach loop="$tamz.Tmod" value="$mod">
<xen:avatar user="$mod" size="m" img="true" />
	<div class="visitorText">
			<xen:username user="$mod" rich="true" />
<div class="userTitle">{xen:helper userTitle, $mod}</div><br />		
			<div class="stats">			
				<dl class="pairsJustified"><dt>{xen:phrase messages}: {xen:number $mod.message_count}</dt></dl>
				<dl class="pairsJustified"><dt>{xen:phrase likes}: {xen:number $mod.like_count}</dt></dl>
				<dl class="pairsJustified"><dt>{xen:phrase points}: {xen:number $mod.trophy_points}</dt></dl>
			</div>
		</div>
</xen:foreach>
<xen:else />
قريباً
</xen:if>
</td></xen:if>

        </tr>
<xen:if is="{$xenOptions.medal_on}">
<tr class="dataRow">
<td><img src="{$xenOptions.medal_1}" alt="" /></td>
<td><img src="{$xenOptions.medal_3}" alt="" /></td>
<xen:if is="{$xenOptions.altmiz_group1}">
<td><img src="{$xenOptions.medal_2}" alt="" /></td></xen:if>
</tr></xen:if>
</tbody>
</table>   
</div></xen:if>]]></template>
    <template title="tama_box.css" version_id="1" version_string="1.1"><![CDATA[table.dataTable {margin: 0;width:100%;}
.heading {margin-bottom: 0;}
.dataRow {text-align:center important!;}
 .tavatarList li
{	
	margin: 5px 0;
	overflow: hidden;
	zoom: 1;	
}
	 .tavatarList .avatar
	{		
		margin-right: 5px;
		float: left;
		width: 96px;
		height: 96px;		
		width: auto;
		height: auto;
	}		
	 .tavatarList .avatar img
	{
		width: 96px;
		height: 96px;
	}	
	 .tavatarList .username
	{		
		font-size: 11pt;
		margin-top: 2px;
		display: block;		
	}	
	 .tavatarList .userTitle
	{		
		color: {xen:property mutedTextColor};		
	}]]></template>
